# VLM API Configuration
VLM_PROVIDER=qwen
VLM_API_KEY=your_api_key_here

# Qwen API (Together AI)
QWEN_ENDPOINT=https://api.together.xyz/v1/chat/completions
QWEN_MODEL=Qwen/Qwen2.5-VL-72B-Instruct

# Nemotron API (NVIDIA)
NEMOTRON_ENDPOINT=https://integrate.api.nvidia.com/v1/chat/completions
NEMOTRON_MODEL=nvidia/nemotron-nano-12b-v2-vl

# Server Configuration
PORT=3000
NODE_ENV=development

# Processing Configuration
UPLOAD_DIR=./uploads
OUTPUT_DIR=./output
CACHE_DIR=./cache
TEMP_DIR=./temp

# Processing Limits
MAX_FILE_SIZE_MB=50
MAX_CONCURRENT_JOBS=5
MAX_PAGES_PER_BATCH=10

# Database
DB_PATH=./data/questions.db

# API Security (optional)
API_KEY=your_api_key_for_client_app
CORS_ORIGIN=*

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/server.log
